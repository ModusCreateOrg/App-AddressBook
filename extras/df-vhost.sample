<VirtualHost *:80>
	ServerName df
	ServerAdmin webmaster@localhost

	DocumentRoot /Users/mschwartz/modus/www/DreamFactory/docroot/
	<Directory />
		Options FollowSymLinks
		AllowOverride None
	</Directory>
	<Directory /Users/mschwartz/modus/www/DreamFactory/docroot/>
		Options All MultiViews
		AllowOverride All
		Order allow,deny
		allow from all
	</Directory>
	<Location /service/>
	    ProxyPass http://modus_create.cloud.dreamfactory.com/
	    ProxyPassReverse http://modus_create.cloud.dreamfactory.com/
	</Location>
	ErrorLog /var/log/apache2/df-error.log
	LogLevel warn
	CustomLog /var/log/apache2/df-access.log combined
</VirtualHost>
